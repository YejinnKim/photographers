<body>

<!-- ======= Header ======= -->
<header id="header" class="fixed-top d-flex align-items-center">
    <div class="container d-flex align-items-center">

        <h1 class="logo me-auto"><a href="/">Photographers</a></h1>
        <!-- Uncomment below if you prefer to use an image logo -->
        <!-- <a href="index.html" class="logo me-auto"><img src="/assets/img/logo.png" alt="" class="img-fluid"></a>-->

        <nav id="navbar" class="navbar">
            <ul>
                <li><a href="/" >Home</a></li>
                <li><a href="/BestPhotographers" >Best Photographers</a></li>
                <li class="dropdown" ><a href="/photos" style="color: #60d923" ><span>Photos</span> <i class="bi bi-chevron-down"></i></a>
                    <ul>
                        <li><a href="/photos"><span>ALL</span></a></li>
                        <li><a href="/photos/category/animal/"><span>Animal</span></a>
                            <!--              <ul>-->
                            <!--                <li><a href="/contents/?ccode=2"><span>aaaaaa</span></a></li>-->
                            <!--              </ul>-->
                        </li>
                        <li><a href="/photos/category/landscape/"><span>Landscape</span></a>

                    </ul>
                </li>
                <% if (!user) { %>
                    <li><a href="/login">Login</a></li>
                <% } else { %>
                    <li><a href="/mypage">My page</a></li>
                    <li><a href="/logout">Logout</a></li>
                <% } %>
            </ul>
            <i class="bi bi-list mobile-nav-toggle"></i>
        </nav><!-- .navbar -->

    </div>
</header>
<!-- End Header -->
<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
        <div class="container">

            <div class="d-flex justify-content-between align-items-center">
                <h2>Photos</h2>
                <ol>
                    <li><a href="/">Home</a></li>
                    <li>ALL</li>
                </ol>
            </div>
            <div class="contact"><button type="button" onclick="location.href='/photos/write/' " class="write-form">글쓰러 가기</button></div>
        </div>
    </section><!-- End Breadcrumbs -->

    <!-- ======= Portfolio Section ======= -->
    <section id="portfolio" class="portfolio">
        <div class="container">
            <div class="row portfolio-container">
                <% data.forEach(function(value) { %>
                    <div class="col-lg-4 col-md-6 portfolio-item filter-app">
                        <div class="portfolio-wrap">
                            <img src="/img/uploadImages/<%= value.thumb_img %>" class="img-fluid" alt="">
                            <a href="/photos/content/<%= value.co_idx %>" class="portfolio-details-lightbox" data-glightbox="type: external" title="Portfolio Details">
                            <div class="portfolio-info">
                                <h4><%= value.subject %></h4>
                            </div>
                            </a>
                        </div>
                    </div>
                <% }); %>
<!--                    <tr>-->
<!--                        <td colspan="7" class="nodata">[현재 등록된 자료가 없습니다.]</td>-->
<!--                    </tr>-->
            </div>
        </div>
    </section><!-- End Portfolio Section -->

</main><!-- End #main -->

